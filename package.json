{
  "name": "anythingzoomer",
  "title": "AnythingZoomer",
  "version": "2.2.7",
  "description": "Zoom in on images or content.",
  "author": {
    "name": "Chris Coyier",
    "url": "https://github.com/chriscoyier"
  },
  "maintainers": [
    {
      "name": "Rob Garrison",
      "url": "https://github.com/Mottie",
      "email": "wowmotty@gmail.com"
    }
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "homepage": "https://css-tricks.com/examples/AnythingZoomer/",
  "bugs": "https://github.com/CSS-Tricks/AnythingZoomer/issues",
  "docs": "http://css-tricks.github.io/AnythingZoomer/use.html",
  "demo": "http://css-tricks.github.io/AnythingZoomer/",
